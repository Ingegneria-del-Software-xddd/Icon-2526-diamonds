@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ex: <http://example.org/diamonds#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:diamond_unknown a ex:Diamond ;
    rdfs:label "Diamante unknown" ;
    ex:beautyCategory "MEDIUM" ;
    ex:fuzzyBeautyScore "0.5"^^xsd:float ;
    ex:hasFeature ex:carat,
        ex:clarity,
        ex:color,
        ex:cut,
        ex:depth,
        ex:table,
        ex:x,
        ex:y,
        ex:z ;
    ex:hasThresholdEvaluation <http://example.org/diamonds#diamond_unknown/eval_carat>,
        <http://example.org/diamonds#diamond_unknown/eval_clarity>,
        <http://example.org/diamonds#diamond_unknown/eval_color>,
        <http://example.org/diamonds#diamond_unknown/eval_cut>,
        <http://example.org/diamonds#diamond_unknown/eval_depth>,
        <http://example.org/diamonds#diamond_unknown/eval_table> ;
    dc:date "2024"^^xsd:gYear .

<http://example.org/diamonds#diamond_unknown/carat_value> a ex:DiamondFeature ;
    ex:featureValue "medium"^^xsd:string .

<http://example.org/diamonds#diamond_unknown/clarity_value> a ex:DiamondFeature ;
    ex:featureValue "high"^^xsd:string .

<http://example.org/diamonds#diamond_unknown/color_value> a ex:DiamondFeature ;
    ex:featureValue "j"^^xsd:string .

<http://example.org/diamonds#diamond_unknown/cut_value> a ex:DiamondFeature ;
    ex:featureValue "good"^^xsd:string .

<http://example.org/diamonds#diamond_unknown/depth_value> a ex:DiamondFeature ;
    ex:featureValue "medium"^^xsd:string .

<http://example.org/diamonds#diamond_unknown/table_value> a ex:DiamondFeature ;
    ex:featureValue "high"^^xsd:string .

<http://example.org/diamonds#diamond_unknown/x_value> a ex:DiamondFeature ;
    ex:featureValue "high"^^xsd:string .

<http://example.org/diamonds#diamond_unknown/y_value> a ex:DiamondFeature ;
    ex:featureValue "low"^^xsd:string .

<http://example.org/diamonds#diamond_unknown/z_value> a ex:DiamondFeature ;
    ex:featureValue "high"^^xsd:string .

<http://example.org/diamonds#diamond_unknown/eval_carat> a ex:QualityThreshold ;
    ex:appliesToFeature ex:carat ;
    ex:expectedOperator "<=" ;
    ex:expectedValue "medium"^^xsd:string ;
    ex:observedValue "medium"^^xsd:string ;
    ex:thresholdRespected true .

<http://example.org/diamonds#diamond_unknown/eval_clarity> a ex:QualityThreshold ;
    ex:appliesToFeature ex:clarity ;
    ex:expectedOperator ">=" ;
    ex:expectedValue "si1"^^xsd:string ;
    ex:observedValue "high"^^xsd:string ;
    ex:thresholdRespected true .

<http://example.org/diamonds#diamond_unknown/eval_color> a ex:QualityThreshold ;
    ex:appliesToFeature ex:color ;
    ex:expectedOperator ">=" ;
    ex:expectedValue "h"^^xsd:string ;
    ex:observedValue "j"^^xsd:string ;
    ex:thresholdRespected true .

<http://example.org/diamonds#diamond_unknown/eval_cut> a ex:QualityThreshold ;
    ex:appliesToFeature ex:cut ;
    ex:expectedOperator ">=" ;
    ex:expectedValue "very_good"^^xsd:string ;
    ex:observedValue "good"^^xsd:string ;
    ex:thresholdRespected true .

<http://example.org/diamonds#diamond_unknown/eval_depth> a ex:QualityThreshold ;
    ex:appliesToFeature ex:depth ;
    ex:expectedOperator "==" ;
    ex:expectedValue "medium"^^xsd:string ;
    ex:observedValue "medium"^^xsd:string ;
    ex:thresholdRespected true .

<http://example.org/diamonds#diamond_unknown/eval_table> a ex:QualityThreshold ;
    ex:appliesToFeature ex:table ;
    ex:expectedOperator "==" ;
    ex:expectedValue "medium"^^xsd:string ;
    ex:observedValue "high"^^xsd:string ;
    ex:thresholdRespected true .

ex:x ex:featureValue "high"^^xsd:string .

ex:y ex:featureValue "low"^^xsd:string .

ex:z ex:featureValue "high"^^xsd:string .

ex:carat ex:featureValue "medium"^^xsd:string .

ex:clarity ex:featureValue "high"^^xsd:string .

ex:color ex:featureValue "j"^^xsd:string .

ex:cut ex:featureValue "good"^^xsd:string .

ex:depth ex:featureValue "medium"^^xsd:string .

ex:table ex:featureValue "high"^^xsd:string .

